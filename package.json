{
  "name": "minyan-times",
  "version": "0.0.01",
  "description": "",
  "main": "index.js",
  "scripts": {
    "preview": "npm-run-all --parallel sync wpstart tailwindwatch",
    "sync": "browser-sync start -p 'local-react.local' --files '**/*.php' 'build/*.js' 'build/*.css'",
    "build": "npm-run-all --sequential buildwp tailwindbuild",
    "buildwp": "wp-scripts build src/*.js",
    "wpstart": "wp-scripts start src/*.js",
    "start": "npm-run-all --parallel wpstart tailwindwatch",
    "tailwindbuild": "tailwindcss -i ./src/styles.css -o ./build/styles.css --postcss --minify",
    "tailwindwatch": "tailwindcss -i ./src/styles.css -o ./build/styles.css --watch --postcss ",
    "packages-update": "wp-scripts packages-update",
    "test": "echo \"0Error: no test specified\" && exit 1",
    "package-release": "mkdir -p wp-minyan-times/build/ && cp build/* wp-minyan-times/build/ && cp -r services index.php block.json wp-minyan-times/",
    "release": "npm-run-all --sequential buildwp tailwindbuild plugin-zip",
    "plugin-zip": "wp-scripts plugin-zip"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@prettier/plugin-php": "^0.19.2",
    "@wordpress/scripts": "^23.3.0",
    "autoprefixer": "^10.4.7",
    "browser-sync": "^2.27.10",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "postcss-nested": "^5.0.6",
    "tailwindcss": "^3.1.4"
  },
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.3.26",
    "@headlessui/react": "^1.7.7",
    "@heroicons/react": "^2.0.13",
    "@tanstack/react-query": "^4.20.4",
    "@tanstack/react-query-devtools": "^4.22.0",
    "axios": "^1.2.2",
    "dayjs": "^1.11.7",
    "google-map-react": "^2.2.0",
    "react": "17.0.0"
  }
}
